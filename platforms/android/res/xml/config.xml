<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.grif0193.first" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>grif0193</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="port-mdpi" src="res/screens/portmdpi.png" />
    <splash density="port-hdpi" src="res/screens/porthdpi.png" />
    <splash density="port-xhdpi" src="res/screens/portxhdpi.png" />
    <splash density="port-xxhdpi" src="res/screens/portxxhdpi.png" />
    <splash density="port-xxxhdpi" src="res/screens/portxxxhdpi.png" />
    <splash density="land-mdpi" src="res/screens/landmdpi.png" />
    <splash density="land-hdpi" src="res/screens/landhdpi.png" />
    <splash density="land-xhdpi" src="res/screens/landxhdpi.png" />
    <splash density="land-xxhdpi" src="res/screens/landxxhdpi.png" />
    <splash density="land-xxxhdpi" src="res/screens/landxxxhdpi.png" />
    <icon density="mdpi" src="res/icons/mdpi.png" />
    <icon density="hdpi" src="res/icons/hdpi.png" />
    <icon density="xhdpi" src="res/icons/xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
