<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.grif0193.first" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>grif0193</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="SplashScreen" value="screen"/>
    <preference name="SplashScreenDelay" value="5000"/>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        
        <splash density="port-mdpi" src="res/screens/portmdpi.png"/>
        <splash density="port-hdpi" src="res/screens/porthdpi.png"/>
        <splash density="port-xhdpi" src="res/screens/portxhdpi.png"/>
        <splash density="port-xxhdpi" src="res/screens/portxxhdpi.png"/>
        <splash density="port-xxxhdpi" src="res/screens/portxxxhdpi.png"/>
        <splash density="land-mdpi" src="res/screens/landmdpi.png"/>
        <splash density="land-hdpi" src="res/screens/landhdpi.png"/>
        <splash density="land-xhdpi" src="res/screens/landxhdpi.png"/>
        <splash density="land-xxhdpi" src="res/screens/landxxhdpi.png"/>
        <splash density="land-xxxhdpi" src="res/screens/landxxxhdpi.png"/>
        
        
        
        <icon src="res/icons/mdpi.png" density="mdpi" />
        <icon src="res/icons/hdpi.png" density="hdpi" />
        <icon src="res/icons/xhdpi.png" density="xhdpi" />
        <icon src="res/icons/xxhdpi.png" density="xxhdpi" />
        <icon src="res/icons/xxxhdpi.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
